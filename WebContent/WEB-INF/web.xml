<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="POCZoneBackend" version="3.0">
  <servlet>
    <servlet-name>POCZoneApplicationServlet</servlet-name>
    <servlet-class>net.poczone.framework.servlet.ApplicationServlet</servlet-class>
    <init-param>
      <param-name>application</param-name>
      <param-value>net.poczone.backend.POCZoneApplication</param-value>
    </init-param>
    <init-param>
      <param-name>path</param-name>
      <param-value>/</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>POCZoneApplicationServlet</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>
</web-app>